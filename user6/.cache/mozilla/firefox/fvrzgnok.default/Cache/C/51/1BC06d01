        //                     }
                     
        window.open("http://services.ptcmysore.gov.in/Speednettracking/Track.aspx?articlenumber=" + Tracking_Number);
        document.getElementById('TxtArticleNumber').focus();
        return false;
        }
        }
        else {
        alert('Sorry, your tracking number is not valid.');
        document.getElementById('TxtArticleNumber').focus();
        document.getElementById('TxtArticleNumber').select();
        return false;
        }

        }

        function validateLogin() {
        if (isBlank(document.getElementById('TxtUserName').value) == true) {
        alert("Please enter the Username");
