                                <tr style="background-color: #FFEFD5;">
                                    <td align="center">
                                        <img id="imgcap" src="captcha.aspx" style="background-color:White;height:32px;width:100px;" />
                                    </td>
                                </tr>
                                <tr style="background-color: #FFEFD5">
                                    <td align="Left">
                                        <span id="Label2" style="font-size:9pt;vertical-align: text-top;">Please enter the text shown in the box below:</span>
                                    </td>
                                </tr>
