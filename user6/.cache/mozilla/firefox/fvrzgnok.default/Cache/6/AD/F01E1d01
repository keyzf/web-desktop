<script language="javascript">
    function Accept_Numbers(e) {
        if ((e.keyCode == 8) || (e.keyCode == 32) || (e.keyCode >= 48 && e.keyCode <= 57)) {

        }
        else {
            e.keyCode = 0;
            return false;
        }
        return true;
    }
    function validateCaptcha() {
        var captcha = document.getElementById("txtCaptcha");
        if (captcha.value == "") {
            alert("Please enter the verification code");
            captcha.focus();
            return false;
        }
        else if (captcha.value != "") {
            var r1 = document.getElementById("r1");
            var r2 = document.getElementById("r2");
            var r3 = document.getElementById("r3");
            if (r1.checked == true) {
                var consignmentNumber = document.getElementById("TxtArticleNumber");
                if (consignmentNumber.value == "" || consignmentNumber.value == "Consignment Number") {
                    alert("Please enter your Consignment Number");
                    consignmentNumber.focus();
                    return false;
                }
            }
            else if (r3.checked == true) {
                var complaintPrefix = document.getElementById("TxtComplaintPrefix");
                var complaintSuffix = document.getElementById("TxtComplaintSuffix");
                if (complaintPrefix.value == "" || complaintSuffix.value == "" || complaintPrefix.value == "Complaint" || complaintSuffix.value == "Number") {
                    alert("Please enter your Complaint Number");
                    complaintPrefix.focus();
                    return false;
                }
            }
            else if (r2.checked == true) {
                var pincode = document.getElementById("TxtPostOffice");
                if (pincode.value == "Pincode or Post office") {
                    alert("Please enter Pincode or Post office");
                    pincode.focus();
                    return false;
                }
            }
        }
    }
</script>
